{
  "name": "airfrydocs",
  "version": "0.0.1",
  "description": "Airfry Static Site Generator Docs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaunt/airfryDocs.git"
  },
  "keywords": [
    "ssg"
  ],
  "author": "Stephen Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaunt/airfryDocs/issues"
  },
  "homepage": "https://github.com/jaunt/airfryDocs#readme",
  "scripts": {
    "airfry": "npx airfry --noWatch",
    "serve": "npx airfry --noWatch && concurrently --kill-others \"npx airfry --watchOnly\" \"npm run dev\"",
    "dev": "vite",
    "build": "npx airfry -nw && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@danglingdev/airfry": "^0.1.0",
    "@tailwindcss/typography": "^0.5.1",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "highlight.js": "^11.5.0",
    "markdown-it": "^13.0.0",
    "markdown-it-fence": "^0.1.3",
    "markdown-it-highlightjs": "^4.0.1",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.18",
    "vite": "^2.7.2"
  }
}
